{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "lib": [
            "dom",
            "esnext",
            "es6"
        ],
        "jsx": "react",
        "jsxFactory": "h",
        "importHelpers": true,
        "strict": false,
        "strictBindCallApply": false,
        "strictFunctionTypes": false,
        "stripInternal": false,
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "allowUnusedLabels": true,
        "noImplicitReturns": false,
        "suppressImplicitAnyIndexErrors": false,
        "baseUrl": ".",
        "outDir": "./dist",
        "rootDir": "./",
        "strictPropertyInitialization": false,
        "noImplicitThis": false,
        "strictNullChecks": false,
        "skipDefaultLibCheck": true,
        "declaration": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": true,
        "noErrorTruncation": true,
        "noImplicitUseStrict": true,
        "emitBOM": false,
        "paths": {
            "ims-decorator": [
                "packages/ims-decorator/lib/index.ts"
            ],
            "nger-core": [
                "packages/nger-core/lib/index.ts"
            ],
            "nger-watcher": [
                "packages/nger-watcher/lib/index.ts"
            ],
            "nger-module-pm2": [
                "packages/nger-module-pm2/lib/index.ts"
            ],
            "nger-module-gulp": [
                "packages/nger-module-gulp/lib/index.ts"
            ],
            "nger-module-webpack": [
                "packages/nger-module-webpack/lib/index.ts"
            ],
            "nger-module-typeorm": [
                "packages/nger-module-typeorm/lib/index.ts"
            ],
            "nger-di": [
                "packages/nger-di/lib/index.ts"
            ],
            "nger-logger": [
                "packages/nger-logger/lib/index.ts"
            ],
            "nger-components": [
                "packages/nger-components/lib/index.ts"
            ],
            "nger-platform-native": [
                "packages/nger-platform-native/lib/index.ts"
            ],
            "nger-platform-ios": [
                "packages/nger-platform-ios/lib/index.ts"
            ],
            "nger-platform-android": [
                "packages/nger-platform-android/lib/index.ts"
            ],
            "nger-platform-cli": [
                "packages/nger-platform-cli/lib/index.ts"
            ],
            "nger-platform-test": [
                "packages/nger-platform-test/lib/index.ts"
            ],
            "nger-platform-express": [
                "packages/nger-platform-express/lib/index.ts"
            ],
            "nger-platform-axios": [
                "packages/nger-platform-axios/lib/index.ts"
            ],
            "nger-platform-weapp": [
                "packages/nger-platform-weapp/lib/index.ts"
            ],
            "nger-platform-koa": [
                "packages/nger-platform-koa/lib/index.ts"
            ],
            "nger-compiler": [
                "packages/nger-compiler/lib/index.ts"
            ],
            "nger-compiler-preact": [
                "packages/nger-compiler-preact/lib/index.ts"
            ],
            "nger-provider-style": [
                "packages/nger-provider-style/lib/index.ts"
            ],
            "nger-compiler-uglify": [
                "packages/nger-compiler-uglify/lib/index.ts"
            ],
            "nger-provider-typescript": [
                "packages/nger-provider-typescript/lib/index.ts"
            ],
            "nger-compiler-img": [
                "packages/nger-compiler-img/lib/index.ts"
            ],
            "nger-compiler-weapp": [
                "packages/nger-compiler-weapp/lib/index.ts"
            ],
            "nger-store": [
                "packages/nger-store/lib/index.ts"
            ],
            "nger-util": [
                "packages/nger-util/lib/index.ts"
            ],
            "nger-ui": [
                "packages/nger-ui/lib/index.ts"
            ],
            "nger-effects": [
                "packages/nger-effects/lib/index.ts"
            ],
            "nger-platform-di": [
                "packages/nger-platform-di/lib/index.ts"
            ],
            "nger-platform-node": [
                "packages/nger-platform-node/lib/index.ts"
            ],
            "nger-platform-style": [
                "packages/nger-provider-style/lib/index.ts"
            ],
            "nger-platform-browser": [
                "packages/nger-platform-browser/lib/index.tsx"
            ],
            "nger-dom": [
                "packages/nger-dom/lib/index.ts"
            ],
            "nger-webpack-admin": [
                "packages/nger-webpack-admin/lib/index.ts"
            ],
            "@babel/core": [
                "node_modules/@types/babel__core"
            ],
            "@babel/generator": [
                "node_modules/@types/babel__generator"
            ],
            "@babel/template": [
                "node_modules/@types/babel__template"
            ],
            "@babel/traverse": [
                "node_modules/@types/babel__traverse"
            ],
            "*": [
                "./node_modules/*",
                "packages/*"
            ],
            "~/*": [
                "app/*"
            ]
        },
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "node",
            "mocha",
            "reflect-metadata"
        ],
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "plugins": [
            {
                "nger": "./dist/nger-language-service/lib/index.js"
            }
        ]
    },
    "include": [
        "packages",
        "framework",
        "addon",
        "framework",
        "typings/*.d.ts",
        "framework/global.d.ts",
        "app"
    ],
    "exclude": [
        "node_modules",
        "platforms",
        "typings",
        "**/*.aot.ts"
    ]
}